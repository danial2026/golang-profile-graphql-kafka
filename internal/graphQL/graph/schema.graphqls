type User {
  id: ID!
  email: String!
  Username: String!
}

input NewUser {
  email: String!
  Username: String!
}

type Post {
  id: ID!
  profileId: String!
  body: String!
}

input NewPost {
  profileId: String!
  body: String!
}


type Query {
  posts: [Post!]!
  users: [User!]!
}

type Mutation {
  createUser(input: NewUser!): User!
  createPost(input: NewPost!): Post!
}
